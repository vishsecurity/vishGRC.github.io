{
  "name": "acquisory-tprm",
  "version": "2.0.0",
  "description": "Acquisory Third-Party Risk Management (TPRM) Platform - Production-ready vendor assessment system",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "prod": "NODE_ENV=production node server.js",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "ngrok": "ngrok http $PORT --authtoken=$NGROK_TOKEN",
    "ngrok:prod": "ngrok http $PORT --authtoken=$NGROK_TOKEN --domain=tprm.yourcompany.ngrok-free.app",
    "lint": "eslint .",
    "test": "jest",
    "clean": "rm -rf uploads/* tprm.db node_modules/.cache"
  },
  "keywords": [
    "tprm",
    "third-party-risk",
    "vendor-assessment",
    "cybersecurity",
    "compliance",
    "risk-management"
  ],
  "author": "Acquisory TPRM Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/acquisory/tprm-platform"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.22.1",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.3",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.1",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^3.1.4"
  },
  "packageManager": "npm@9.8.1",
  "volta": {
    "node": "20.11.1"
  }
}
